import java.util.Scanner;

public class Main {

	
	public static void main(String[] args) {
		

		// Write your code here
		Scanner s = new Scanner(System.in);
		int x1 = s.nextInt();
        int y1 = s.nextInt();
		int x2 = s.nextInt();
		int y2 = s.nextInt();
		int a = ((y2-y1)*(x2-x1));
		System.out.println(a);

	}

}
